.list
      background-color #F8F8F8  
      padding-top .1rem
      ul
        overflow hidden
        padding 0 .04rem
        li
          width 47%
          height 2.3rem
          background-color #fff
          float left
          margin  0 .05rem
          margin-bottom 0.05rem
          box-sizing border-box
          .shopImg
            height 70%
            width 100%
            img
              width 100%
              height 100%
          .shopTxt
            height 30%
            padding-left 0.1rem
            .shopName
              height .18rem
              line-height .18rem
              font-size .14rem
              color #262626
              font-weight 700
              margin-bottom .05rem
            .shopDes
              text-overflow ellipsis
              white-space nowrap
              word-break break-all
              width 100%
              display block
              display block
              font-size .12rem
              line-height .15rem
              overflow hidden
              color #8c8c8c
              margin-bottom .05rem
            .pricList
              display flex
              justify-content space-between
              align-items center
              .price
                font-size .14rem
                color #ea413c
                font-weight 700
              span
                font-size .14rem
                width .19rem
                height .16rem
                padding-right 0.1rem
    