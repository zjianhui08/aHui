.icon-cart1 
  color #EE6C68
  font-size .16rem
.list
  padding-top .1rem
  ul
    overflow hidden
    padding 0 .04rem
    li
      width 46.95%
      height 2.3rem
      background-color #fff
      float left
      margin  0 .05rem
      margin-bottom 0.05rem
      box-sizing border-box
      .shopImg
        height 70%
        width 100%
        img
          width 100%
          height 100%
      .shopTxt
        height 30%
        .shopName
          height .18rem
          line-height .18rem
          font-size .13rem
          color #262626
          margin-bottom .05rem
          text-overflow ellipsis
          overflow hidden
          white-space nowrap
        .shopDes
          text-overflow ellipsis
          white-space nowrap
          word-break break-all
          width 100%
          display block
          display block
          font-size .106rem
          line-height .15rem
          overflow hidden
          color #8c8c8c
          margin-bottom .05rem
        .pricList
          display flex
          justify-content space-between
          align-items center
          .price
            font-size .14rem
            color #ea413c
            font-weight 700
          span
            font-size .14rem
            width .19rem
            height .16rem
            padding-right 0.1rem